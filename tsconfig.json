{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true
  },
  "baseUrl": "./app/",
  "paths": {
    "@/components/*": [
      "components/*"
    ],
    "@/navigation/*": [
      "navigation/*"
    ],
    "@/assets/*": [
      "assets/*"
    ],
    "@/hooks/*": [
      "hooks/*"
    ],
    "@/types/*": [
      "types/*"
    ],
    "@/utils/*": [
      "utils/*"
    ],
    "@/store/*": [
      "store/*"
    ]
  }
}
